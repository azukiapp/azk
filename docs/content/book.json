{
  "title": "azk Docs Gitbook",
  "description": "Gitbook documentation for azk",
  "version": "2.6.4",
  "azkVersion": "0.13.0",
  "releaseNotes": "http://......",
  "plugins": [
    "ga",
    "hotjar",
    "include",
    "scrollablecode",
    "-sharing",
    "github",
    "sitemap",
    "anchors"
  ],
  "pluginsConfig": {
    "hotjar": {
      "hjid": 12345678901
    },
    "ga": {
      "token": "GA_UA_ID",
      "configuration": {
        "legacyCookieDomain": "GA_LEGACY_COOKIE_DOMAIN"
      }
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": []
    },
    "github": {
      "url": "https://github.com/azukiapp/azk"
    },
    "sitemap": {
      "hostname": "http://docs.azk.io/"
    }
  },
  "links": {
    "home": "http://azk.io",
    "issues": "https://github.com/azukiapp/azk/issues",
    "theme": "node_modules/gitbook-theme-clarity/lib",
    "custom": {
        "custom": "http://azukiapp.com"
    }
  }
}
